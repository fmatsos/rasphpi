{
    "name": "fmatsos/rasphpi",
    "description": "PHP library to access to communicate with interfaces from your Rpi.",
    "keywords": ["raspberry", "rpi"],
    "type": "library",
    "license": "AGPL-3.0-or-later",
    "authors": [
        {
            "name": "Franck Matsos",
            "email": "franck@matsos.fr"
        }
    ],
    "require": {
        "php": "^8.0",
        "illuminate/collections": "^8.20"
    },
    "autoload": {
        "psr-4": { "Rasphpi\\": "src/" },
        "classmap": ["src/"]
    },
    "autoload-dev": {
        "psr-4": { "Rasphpi\\Tests\\": "tests/" }
    },
    "config": {
        "sort-packages": true
    }
}
